

<div class="row">
	<div class="col-md-6 col-md-offset-3">

		<%= form_for([@listing, @reservation]) do |f| %>

		<%= f.label :Start_Date %>
		<%= f.date_field :start_time, class: 'form-control' %>

		<%= f.label :End_Date %>
		<%= f.date_field :end_time, class: 'form-control' %>

		<br>

		<%= f.submit "Create Reservation", class: "btn btn-primary" %>

		<% end %>

	</div>
</div>

